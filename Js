// Example: Update dashboard stats with data from API
fetch('/api/dashboard-stats')
  .then(response => response.json())
  .then(data => {
    document.getElementById('total-users').textContent = data.totalUsers;
    document.getElementById('total-deposits').textContent = data.totalDeposits;
    document.getElementById('total-withdrawals').textContent = data.totalWithdrawals;
  })
  .catch(err => console.error(real 
